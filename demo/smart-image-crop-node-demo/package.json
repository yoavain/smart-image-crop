{
    "name": "smart-image-crop-node-demo",
    "version": "1.0.0",
    "description": "Test page for @yoavain/smart-image-crop",
    "private": true,
    "scripts": {
        "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
        "start:server": "nodemon",
        "start:server:debug": "cross-env TS_NODE_TRANSPILE_ONLY=true node --inspect-brk --require ts-node/register src/server/server.ts",
        "start:client": "parcel watch src/client/index.html --out-dir=public",
        "type-check": "tsc --build tsconfig.json",
        "eslint": "eslint src/",
        "eslint:fix": "npm run eslint -- --fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "engines": {
        "node": ">=12 <17",
        "npm": "<9"
    },
    "dependencies": {
        "@codait/max-vis": "0.3.0",
        "axios": "0.27.2",
        "fastify": "4.8.1",
        "fastify-compress": "4.1.0",
        "fastify-helmet": "7.1.0",
        "fastify-multipart": "5.4.0",
        "fastify-static": "4.7.0",
        "make-promises-safe": "5.1.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "rimraf": "3.0.2"
    },
    "devDependencies": {
        "@types/busboy": "1.5.0",
        "@types/node": "18.7.12",
        "concurrently": "7.3.0",
        "cross-env": "7.0.3",
        "eslint": "8.22.0",
        "nodemon": "2.0.19",
        "parcel-bundler": "1.12.5",
        "rimraf": "3.0.2",
        "ts-node": "10.9.1",
        "typescript": "4.7.4"
    }
}
