{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "target": "es6",
    "lib": ["dom", "es6", "es2017", "es2017.object", "esnext.asynciterable"],
    "rootDir": ".",
    "noEmit": true,
    "importsNotUsedAsValues": "error"
  },
  "include": [
      "../demo/smart-image-crop-browser-demo/src/**/*", "test/**/*"],
  "exclude": ["node_modules"]
}
